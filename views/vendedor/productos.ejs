            </div>
          </div>
          <form class="form-inline my-2 my-lg-0 ml-0 ml-lg-4 mb-3 mb-lg-0">
            <button class="btn  my-2 my-sm-0 nav_search-btn" type="submit"></button>
          </form>
        </nav>
      </div>
    </header>
  </div>
  <!-- end header section -->
  <!-- slider section -->
  <div class="container">
    <h1>Tabla de Productos</h1>
    <table class="table">
      <thead>
        <tr>
          <th>Codigo</th>
          <th>Descripcion</th>
          <th>Precio</th>
          <th>Stock</th>
          <th>Categoria</th>
          <!-- Agrega más encabezados de columna si es necesario -->
        </tr>
      </thead>
      <tbody>
        <% productos.forEach(function(producto) { %>
          <tr>
            <td>
              <%= producto.codigo %>
            </td>
            <td>
              <%= producto.descripcion %>
            </td>
            <td>
              <%= producto.precio %>
            </td>
            <td>
              <%= producto.stock %>
            </td>
            <td>
              <%= producto.categoria %>
            </td>
          </tr>
          <% }); %>
      </tbody>
    </table>
  </div>
