      <div class="container-fluid">
        <nav class="navbar navbar-expand-lg custom_nav-container ">
          <a class="navbar-brand" href="/admin/">
            <span>
              Music pro /admin
            </span>
          </a>
          <button class="navbar-toggler ml-auto" type="button" data-toggle="collapse"
            data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
            aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>

          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <div class="d-flex mx-auto flex-column flex-lg-row align-items-center">
              <ul class="navbar-nav  ">
                <li class="nav-item">
                  <a class="nav-link" href="/admin/">Inicio </a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" href="/admin/crear-usuario"> Crear usuario </a>
                </li>
                <li class="nav-item active">
                  <a class="nav-link" href="/listar-usuario"> Usuarios <span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" href="/admin">Pedidos</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" href="/">Cerrar sesion</a>
                </li>
              </ul>
            </div>
          </div>
          <form class="form-inline my-2 my-lg-0 ml-0 ml-lg-4 mb-3 mb-lg-0">
            <button class="btn  my-2 my-sm-0 nav_search-btn" type="submit"></button>
          </form>
        </nav>
      </div>
    </header>
    <!-- end header section -->
    <!-- contact section -->
    <div class="container">
        <h1>Tabla de Usuario</h1>
        <table class="table table-striped-columns">
          <thead>
            <tr>
              <th>ID</th>
              <th>Nombre</th>
              <th>Apellido</th>
              <th>Correo Electronico</th>
              <th>Usuario</th>
              <th>Contraseña</th>
              <th>TipoUsuario</th>
              <th>Acciones</th>
            </tr>
          </thead>
          <tbody>
            <% empleado.forEach(function(empleado) { %>
              <tr>
                <td><%= empleado.id %></td>
                <td><%= empleado.nombre%> </td>
                <td><%= empleado.apellido%> </td>
                <td><%= empleado.correo %></td>
                <td><%= empleado.user %></td>
                <td><%= empleado.pass %></td>
                <td><%= empleado.tipoUsuario %></td>
                <td>
                  <a href="/editar-usuario1/<%= empleado.id %>">Editar</a>
                  <a href="/eliminar-empleados/<%= empleado.id %>" onclick="return confirm('¿Estás seguro de eliminar este empleado?')">Eliminar</a> 
                </td>
              </tr>
            <% }); %>
          </tbody>
        </table>
      </div>
